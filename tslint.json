{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "double"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-bitwise": false,
    "trailing-comma": false,
    "array-type": [true, "array"]
  },
  "rulesDirectory": []
}
